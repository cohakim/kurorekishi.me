#status-modal.remodal{"data-remodal-id" => "modal"}
  %table
    %thead
      %tr
        %th.caption{:colspan => "2"} Server
    %tbody
      %tr
        %th 混雑度
        %td
      %tr
        %th 処理中
        %td 3 users
  %table
    %thead
      %tr
        %th.caption{:colspan => "2"} Job
    %tbody
      %tr
        %th 処理対象
        %td {this.props.job.collect_count} 件
      %tr
        %th 削除済
        %td {this.props.job.destroy_count} 件
  %table
    %thead
      %tr
        %th.caption{:colspan => "2"} Parameters
    %tbody
      %tr
        %th Name
        %td {this.props.job_params.user_name}
      %tr
        %th From
        %td {this.props.job_params.from}
      %tr
        %th To
        %td {this.props.job_params.to}
      %tr
        %th
          Protect
          %br>/
          Reply
        %td {String(this.props.job_params.protect_reply)}
      %tr
        %th
          Protect
          %br>/
          Favorite
        %td {String(this.props.job_params.protect_favorite)}
      %tr
        %th Created At
        %td {this.props.job_params.created_at}
  %a.remodal-confirm{"data-remodal-action" => "confirm"} OK

